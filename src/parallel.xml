    <file>
        <name>gml_GlobalScript_scr_fusion_queue</name>
        <init>fusionIngredientName1[fusioncount] = "--"</init>
        <init>fusionIngredientName2[fusioncount] = "--"</init>
        <init>fusionResultName[fusioncount] = "--"</init>
        <init>fusionResultTopComment[fusioncount] = "--"</init>
        <exchange>fusionIngredientName1[fusioncount] = armornametemp</exchange>
        <exchange>fusionIngredientName2[fusioncount] = armornametemp</exchange>
        <exchange>fusionResultName[fusioncount] = armornametemp</exchange>
        <exchange>fusionResultTopComment[fusioncount] = armordesctemp</exchange>
    </file>
    <file>
        <name>gml_Object_obj_npc_hammerguy_Create_0</name>
        <init>fusionResultName = "ITEM"</init>
    </file>
    <file>
        <name>gml_Object_obj_fusionmenu_Step_0</name>
        <init>optionText[1][j] = " "</init>
        <init>optionText[_n][j] = " "</init>
        <init>optionTopComment[i][j] = " "</init>
        <exchange>obj_npc_hammerguy.fusionResultName = fusionResultName[menuCoord[0]]</exchange>
        <exchange>optionCommentB[0][j] = fusionIngredientName2[j]</exchange>
        <exchange>optionCommentA[0][j] = fusionIngredientName1[j]</exchange>
        <exchange>optionText[0][j] = fusionResultName[j]</exchange>
        <exchange>optionTopComment[0][j] = fusionResultTopComment[j]</exchange>
    </file>
    <file>
        <name>gml_Object_obj_npc_hammerguy_Step_0</name>
        <draw>
            <line>msgsetsubloc(0, "* (You got ~1!)/%", fusionResultName, "obj_npc_hammerguy_slash_Step_0_gml_251_0\)</line>
            <variable>fusionResultName</variable>
        </draw>
    </file>
    <file>
        <name>gml_Object_obj_custommenu_Create_0</name>
        <init>optionCommentA[i][j] = " "</init>
        <init>optionCommentB[i][j] = " "</init>
        <init>optionText[i][j] = "---"</init>
    </file>
    <file>
        <name>gml_Object_obj_custommenu_Draw_0</name>
        <draw>
            <line>draw_text_transformed((textx + optionCommentAXOffset[m][j]), (texty + optionCommentAYOffset[m][j]), string(optionCommentA[m][j]), textxscale, textyscale, 0)</line>
            <variable>optionCommentA[m][j]</variable>
        </draw>
        <draw>
            <line>draw_text_transformed((textx + optionCommentBXOffset[m][j]), (texty + optionCommentBYOffset[m][j]), string(optionCommentB[m][j]), textxscale, textyscale, 0)</line>
            <variable>optionCommentB[m][j]</variable>
        </draw>
        <draw>
            <line>draw_text_transformed(textx, texty, string_hash_to_newline(optionText[m][j]), textxscale, textyscale, 0)</line>
            <variable>optionText[m][j]</variable>
        </draw>
        <draw>
            <line>draw_text(((xx + 18) + x_offset), (yy + 20), string_hash_to_newline(optionTopComment[m][menuCoord[m]]))</line>
            <variable>optionTopComment[m][menuCoord[m]]</variable>
        </draw>
        <draw>
            <line>draw_text((xx + 20), (yy + 20), string_hash_to_newline(optionTopComment[m][menuCoord[m]]))</line>
            <variable>optionTopComment[m][menuCoord[m]]</variable>
        </draw>
    </file>
    <file>
        <name>gml_GlobalScript_scr_shopmenu</name>
        <exchange>_itempname = armorname[i]</exchange>
        <draw>
            <line>draw_text(60, (260 + (j * 40)), string_hash_to_newline(_itempname))</line>
            <variable>_itempname</variable>
        </draw>
    </file>
    <file>
        <name>gml_GlobalScript_scr_itemcomment</name>
