            <variable>wmsg</variable>
        </add-msg>
    </file>
    <file>
        <name>gml_Object_obj_menuwriter_Create_0</name>
        <init>msg = " "</init>
    </file>
    <file>
        <name>gml_Object_obj_menuwriter_Draw_0</name>
