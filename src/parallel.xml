            <line>draw_text(x, y, string_hash_to_newline(msg))</line>
            <variable>msg</variable>
        </draw>
    </file>
    <file>
        <name>gml_Object_obj_darkcontroller_Step_0</name>
        <init>wmsg = " "</init>
        <exchange>wmsg = amessage2temp</exchange>
        <exchange>wmsg = amessage3temp</exchange>
        <exchange>wmsg = amessage4temp</exchange>
    </file>
    <file>
        <name>gml_Object_obj_darkcontroller_Draw_0</name>
        <exchange>_abilitytext[arno] = armorability[cur]</exchange>
        <exchange>_abilitytext[1] = chararmor1ability[charcoord]</exchange>
        <exchange>_abilitytext[2] = chararmor2ability[charcoord]</exchange>
        <draw>
            <line>draw_text((xx + 384), ((yy + 230) + (j * ch_vspace)), string_hash_to_newline(armorname[i]))</line>
            <variable>armorname[i]</variable>
        </draw>
        <draw>
            <line>draw_text((xx + 365), (yy + 142), string_hash_to_newline(chararmor1name[charcoord]))</line>
            <variable>chararmor1name[charcoord]</variable>
        </draw>
        <draw>
            <line>draw_text((xx + 365), (yy + 172), string_hash_to_newline(chararmor2name[charcoord]))</line>
            <variable>chararmor2name[charcoord]</variable>
        </draw>
        <draw>
            <line>draw_text((xx + 20), (yy + 10), string_hash_to_newline(chararmor1desc[charcoord]))</line>
            <variable>chararmor1desc[charcoord]</variable>
        </draw>
        <draw>
            <line>draw_text((xx + 20), (yy + 10), string_hash_to_newline(chararmor2desc[charcoord]))</line>
            <variable>chararmor2desc[charcoord]</variable>
        </draw>
        <draw>
            <line>draw_text((xx + 100), ((yy + 230) + (ch_vspace * (i + 3))), string_hash_to_newline(_abilitytext[i]))</line>
            <variable>_abilitytext[i]</variable>
        </draw>
        <draw>
            <line>draw_text((xx + 100), ((yy + 230) + (ch_vspace * (i + 3))), string_hash_to_newline(_abilitytext[i]))</line>
            <variable>_abilitytext[i]</variable>
        </draw>
    </file>
    <file>
        <name>gml_GlobalScript_scr_itemget_anytype_text</name>
        <assignment>
            <line>itemgetstring = stringsetsubloc("* (\\cY~1\\cW was added to your \\cY~2\\cW.)", itemname, itemtypename, "obj_treasure_room_slash_Other_10_gml_76_0")</line>
